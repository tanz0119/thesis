%   |      \end{minipage}%| \\
%   |      \caption{Second subcaption.}%| \\
%   |    \end{subfloat}| \\
%   |  \end{center}| \\
%   |  \caption{This is an example of verbatim text in a subfigure.}| \\
%   |\end{figure}|
% \end{quote}
